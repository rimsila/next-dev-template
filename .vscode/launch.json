{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\scripts\\module",
      "args": ["add", "next-dev-template", "@wetrial/project"],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\node_modules\\eslint\\bin\\eslint.js",
      "args": ["--cache", "--ext", ".js,.jsx,.ts,.tsx", "--format=pretty", "./"],
      "sourceMaps": true
    }
  ]
}
